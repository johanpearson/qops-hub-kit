{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "serviceName": {
      "value": "profile"
    },
    "environment": {
      "value": "dev"
    },
    "location": {
      "value": "swedencentral"
    },
    "jwtSecret": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{rg-name}/providers/Microsoft.KeyVault/vaults/{kv-name}"
        },
        "secretName": "jwt-secret"
      }
    },
    "enableCosmosDb": {
      "value": false
    },
    "enableSqlDatabase": {
      "value": true
    },
    "databaseType": {
      "value": "postgresql"
    },
    "sqlDatabaseName": {
      "value": "profiledb"
    },
    "sqlAdminUsername": {
      "value": "sqladmin"
    },
    "sqlAdminPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/{rg-name}/providers/Microsoft.KeyVault/vaults/{kv-name}"
        },
        "secretName": "sql-admin-password"
      }
    },
    "functionAppSku": {
      "value": "Y1"
    }
  }
}
